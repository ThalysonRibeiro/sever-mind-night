{
  "name": "sever-mind-night",
  "version": "1.0.0",
  "description": "API for Sever Mind Night",
  "main": "dist/server.js",
  "scripts": {
    "test": "jest --testPathIgnorePatterns=tests/e2e",
    "test:watch": "jest --watch --testPathIgnorePatterns=tests/e2e",
    "test:coverage": "jest --coverage --testPathIgnorePatterns=tests/e2e",
    "test:ci": "jest --ci --coverage --watchAll=false --testPathIgnorePatterns=tests/e2e",
    "test:routes": "jest --testPathPatterns=tests/routes",
    "test:auth": "jest --testPathPatterns=tests/routes/auth",
    "test:plugins": "jest --testPathPatterns=tests/plugins",
    "test:utils": "jest --testPathPatterns=tests/utils",
    "test:e2e": "dotenv -e .env.e2e -- jest -c jest.config.e2e.cjs --runInBand",
    "test:e2e:migrate": "dotenv -e .env.e2e -- prisma migrate deploy",
    "test:e2e:start": "docker-compose up -d test-db",
    "test:e2e:stop": "docker-compose stop test-db",
    "test:e2e:clean": "docker-compose down -v",
    "test:all": "npm run test && npm run test:e2e",
    "start": "node --env-file .env --experimental-strip-types --no-warnings src/server.ts",
    "dev": "node --env-file .env --experimental-strip-types --no-warnings --watch src/server.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev"
  },
  "keywords": [
    "fastify",
    "prisma",
    "oauth",
    "jwt",
    "redis",
    "modular"
  ],
  "author": "Thalyson Ribeiro",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/multipart": "^9.0.3",
    "@fastify/oauth2": "^8.1.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/session": "^11.1.0",
    "@fastify/swagger": "^9.5.1",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.12.0",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.1",
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "fastify": "^5.4.0",
    "fastify-swagger": "^5.1.1",
    "fastify-type-provider-zod": "^5.0.2",
    "google-auth-library": "^10.2.0",
    "minimatch": "^10.0.3",
    "prisma": "^6.13.0",
    "reflect-metadata": "^0.2.2",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^7.4.2",
    "jest": "^30.0.5",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
