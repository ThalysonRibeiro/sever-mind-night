{
  "name": "sever-mind-night",
  "version": "1.0.0",
  "description": "API for Sever Mind Night",
  "main": "dist/server.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:route": "jest --testNamePattern",
    "test:quick": "jest --bail --verbose",
    "start": "node --env-file .env --experimental-strip-types --no-warnings src/server.ts",
    "dev": "node --env-file .env --experimental-strip-types --no-warnings --watch src/server.ts",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev"
  },
  "keywords": [
    "fastify",
    "prisma",
    "oauth",
    "jwt",
    "redis",
    "modular"
  ],
  "author": "Thalyson Ribeiro",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/jwt": "^9.1.0",
    "@fastify/multipart": "^9.0.3",
    "@fastify/oauth2": "^8.1.2",
    "@fastify/rate-limit": "^10.3.0",
    "@fastify/session": "^11.1.0",
    "@fastify/swagger-ui": "^5.2.3",
    "@prisma/client": "^6.12.0",
    "@upstash/ratelimit": "^2.0.5",
    "@upstash/redis": "^1.35.1",
    "bcrypt": "^6.0.0",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "fastify": "^5.4.0",
    "fastify-type-provider-zod": "^5.0.2",
    "google-auth-library": "^10.1.0",
    "minimatch": "^10.0.3",
    "prisma": "^6.12.0",
    "reflect-metadata": "^0.2.2",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.0.15",
    "@types/supertest": "^6.0.3",
    "jest": "^30.0.5",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
